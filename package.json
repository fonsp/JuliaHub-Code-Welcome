{
    "name": "juliahub-welcome",
    "displayName": "JuliaHub-Welcome",
    "description": "Welcome page for code instances on JuliaHub",
    "version": "0.3.0",
    "publisher": "JuliaComputing",
    "bugs": "https://github.com/JuliaComputing/JuliaHub/issues",
    "homepage": "https://juliahub.com",
    "license": "SEE LICENSE IN LICENSE.md",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "JuliaHub Welcome",
            "properties": {
                "juliahub-welcome.show": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show JuliaHub welcome screen."
                }
            }
        },
        "commands": {
            "command": "juliahub-welcome.show",
            "title": "JuliaHub: Show welcome page"
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "@iarna/toml": "^2.2.5"
    },
    "extensionDependencies": [
        "julialang.language-julia"
    ]
}
